{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../ui"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.rgba",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "Crypto Sender",
  "mainBinaryName": "Crypto Sender",
  "version": "0.1.0",
  "identifier": "com.Crypto.dev.windows",
  "plugins": {
    "updater": {
      "endpoints": [],
      "dialog": true,
      "pubkey": "",  
      "windows": {
        "installMode": "passive"
      }
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "width": 1300,
        "resizable": true,
        "url": "signin.html",
        "title": "Crypto Sender",
        "minHeight": 600,
        "minWidth": 800,
        "visible": false,
        "center": true,
        "decorations": false,
        "useHttpsScheme": true
      },
      {
        "width": 400,
        "height": 200,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "center": true,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  }

  
}